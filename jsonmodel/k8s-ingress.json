{
  "apiVersion": "extensions/v1beta1",
  "kind": "Ingress",
  "metadata": {
    "annotations": {
      "nginx.ingress.kubernetes.io/rewrite-target": "/"
    },
    "name": "microservice-redis",
    "namespace": "kube-system"
  },
  "spec": {
    "rules": [
      {
        "host": "redis.csse.com.cn",
        "http": {
          "paths": [
            {
              "path": "/",
              "backend": {
                "serviceName": "microservice-redis",
                "servicePort": 6379
              }
            }
          ]
        }
      }
    ]
  }
}